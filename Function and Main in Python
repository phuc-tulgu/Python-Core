def print_even_number(lst):
  lsteven = []
  for i in lst:
    if i % 2 == 0:
      lsteven.append(i)
  print(lsteven)


def nhapmang(lst,n):
  for i in range(n):
        lst.append(int(input()))
  return lst


def main():
   lst = []
   n = int(input())
   nhapmang(lst,n)
   print_even_number(lst)
main()
